<!DOCTYPE html>
<html>
<head>
  <link rel="stylesheet" href="/style/style.css">
</head>
<body>
  <div class="smallBox">
    <p class="pStyle">Home</p>
  </div>

  <div class="mainBox">



    <% Data.forEach(function(lists){ %>

      <form class="form1" action"/" method="post">
      <input name="checkBoxName" type="checkbox" value="<%= lists._id %>" onChange="this.form.submit()"></input>
      <p class="pStyle"> <%= lists.itsList %> </p>

      </form>

  <% }); %>

<form class="form2" action="/post" method="post">
<input type="text" name="addedData" autofocus></input>
<input type="hidden" value="<%= route %>"></input>
<button type="submit" class="from2Button">+</button>
</form>
  </div>
</body>
</html>
